{
  "name": "react-redux-webpack3-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 3 Boilerplate for React + Redux",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV=production || SET \"NODE_ENV=production\" && webpack -p --optimize-minimize && node server.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "clear": "rm -rf ./public",
    "build:dev": "rm -rf ./public && webpack",
    "build:dev--watch": "webpack -d -w",
    "build": "export NODE_ENV=production || SET \"NODE_ENV=production\" && rm -rf ./public && webpack -p",
    "lint": "eslint **/*.{js, jsx} --quiet",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"src/**/*.{js, jsx}\"",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha ./src/__test__/**/*.test.jsx --require babel-register",
    "test:watch": "yarn run test --watch",
    "flow:init": "flow init",
    "flow:status": "flow status",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:coverage": "flow coverage",
    "flow:all": "flow check --all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luigi055/webpack3-React-Redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router-v4",
    "webpack2",
    "boilerplate"
  ],
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "7.1.5",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "4.1.2",
    "css-loader": "0.28.7",
    "css-mqpacker": "6.0.1",
    "enzyme": "3.1.0",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "flow-bin": "0.56.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "image-webpack-loader": "3.4.2",
    "mocha": "4.0.1",
    "node-sass": "^4.5.0",
    "pixrem": "4.0.1",
    "postcss-loader": "2.0.7",
    "postcss-will-change": "2.0.0",
    "prettier": "1.7.4",
    "react-hot-loader": "3.0.0",
    "react-test-renderer": "16.0.0",
    "resolve-url-loader": "2.1.1",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.2",
    "sinon": "4.0.1",
    "sinon-chai": "2.14.0",
    "static-loader": "^0.1.8",
    "style-loader": "0.19.0",
    "stylelint": "8.2.0",
    "stylelint-config-standard": "17.0.0",
    "url-loader": "0.6.2",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "4.0.0-beta",
    "enzyme-adapter-react-16": "1.0.1",
    "express": "4.16.2",
    "gravatar": "^1.6.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.5",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-date-picker": "^6.2.2",
    "react-dom": "16.0.0",
    "react-router-dom": "4.2.2",
    "uuid": "^3.1.0"
  }
}
